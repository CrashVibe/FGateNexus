{
    "name": "fgatenexus",
    "private": true,
    "version": "0.0.2",
    "apiVersion": "0.0.1",
    "minimumClientVersion": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "build:all": "bun run build && bun run build:mac && bun run build:linux && bun run build:win && bun run copy:migrations",
        "build:mac": "bun build --compile --target=bun-darwin-arm64 ./entry.ts --outfile ./dist/FGateNexus-mac-arm64 && bun build --compile --target=bun-darwin-x64 ./entry.ts --outfile ./dist/FGateNexus-mac-x64",
        "build:linux": "bun build --compile --target=bun-linux-arm64 ./entry.ts --outfile ./dist/FGateNexus-linux-arm64 && bun build --compile --target=bun-linux-x64 ./entry.ts --outfile ./dist/FGateNexus-linux-x64",
        "build:win": "bun build --compile --target=bun-windows-x64 ./entry.ts --outfile ./dist/FGateNexus-win-x64.exe",
        "copy:migrations": "node -e \"const fs=require('fs'); if(fs.existsSync('./migrations')) { fs.cpSync('./migrations', './dist/migrations', {recursive: true}); }\""
    },
    "overrides": {
        "@koishijs/loader": "npm:@mrlingxd/loader@^4.6.8-fix.1"
    },
    "dependencies": {
        "@koishijs/plugin-http": "^0.6.3",
        "@koishijs/plugin-logger": "^2.6.9",
        "@koishijs/plugin-server": "^3.2.7",
        "@nuxt/eslint": "^1.11.0",
        "@nuxtjs/tailwindcss": "7.0.0-beta.1",
        "@types/bcryptjs": "^3.0.0",
        "@vicons/ionicons5": "^0.13.0",
        "@vueuse/core": "^14.1.0",
        "bcryptjs": "^3.0.3",
        "better-sqlite3": "^12.5.0",
        "crossws": "0.4.1",
        "drizzle-kit": "^0.31.8",
        "drizzle-orm": "^0.45.0",
        "highlight.js": "^11.11.1",
        "http-status-codes": "^2.3.0",
        "koishi": "^4.18.9",
        "koishi-plugin-adapter-onebot": "npm:@mrlingxd/koishi-plugin-adapter-onebot@^6.8.1",
        "moment-timezone": "^0.6.0",
        "naive-ui": "^2.43.2",
        "nuxt": "^4.2.1",
        "nuxtjs-naive-ui": "1.0.2",
        "otplib": "^12.0.1",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.3",
        "uuid": "^13.0.0",
        "vooks": "^0.2.12",
        "vue": "^3.5.25",
        "vue-router": "^4.6.3",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/bun": "^1.3.3",
        "@types/less": "~3.0.8",
        "@types/node": "^24.10.1",
        "baseline-browser-mapping": "^2.9.2",
        "eslint": "^9.39.1",
        "less": "^4.4.2",
        "prettier": "^3.7.4",
        "typescript": "^5.9.3",
        "unplugin-auto-import": "^20.3.0",
        "unplugin-vue-components": "^30.0.0",
        "vue-tsc": "^3.1.5"
    }
}
