{
  "name": "fgatenexus",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production nuxt build",
    "dev": "NODE_ENV=development bun --bun nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "oxfmt .",
    "lint": "oxlint --type-aware && eslint .",
    "lint:fix": "oxlint --fix --type-aware && eslint . --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "build:all": "NUXT_BUILD_TARGET=binary bun run build && bun run build:mac && bun run build:linux && bun run build:win && bun run copy:migrations",
    "build:mac": "bun build --compile --target=bun-darwin-arm64 ./entry.ts --outfile ./dist/FGateNexus-mac-arm64 && bun build --compile --target=bun-darwin-x64 ./entry.ts --outfile ./dist/FGateNexus-mac-x64",
    "build:linux": "bun build --compile --target=bun-linux-arm64 ./entry.ts --outfile ./dist/FGateNexus-linux-arm64 && bun build --compile --target=bun-linux-x64 ./entry.ts --outfile ./dist/FGateNexus-linux-x64",
    "build:win": "bun build --compile --target=bun-windows-x64 ./entry.ts --outfile ./dist/FGateNexus-win-x64.exe",
    "copy:migrations": "node -e \"const fs=require('fs'); if(fs.existsSync('./migrations')) { fs.cpSync('./migrations', './dist/migrations', {recursive: true}); }\""
  },
  "dependencies": {
    "@koishijs/plugin-http": "^0.6.3",
    "@koishijs/plugin-logger": "^2.6.9",
    "@koishijs/plugin-server": "^3.2.7",
    "@mrlingxd/koishi-plugin-adapter-onebot": "^0.2.8",
    "@nuxt/eslint": "^1.15.2",
    "@nuxt/image": "2.0.0",
    "@nuxt/ui": "^4.5.0",
    "@pinia/nuxt": "^0.11.3",
    "@tailwindcss/vite": "^4.2.1",
    "@types/bcryptjs": "^3.0.0",
    "@vicons/ionicons5": "^0.13.0",
    "@vueuse/core": "^14.2.1",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "bcryptjs": "^3.0.3",
    "crossws": "0.4.4",
    "drizzle-kit": "^0.31.9",
    "drizzle-orm": "^0.45.1",
    "highlight.js": "^11.11.1",
    "http-status-codes": "^2.3.0",
    "koishi": "^4.18.10",
    "limiter": "^3.0.0",
    "lodash-es": "^4.17.23",
    "lru-cache": "^11.2.6",
    "moment-timezone": "^0.6.0",
    "naive-ui": "^2.43.2",
    "nuxt": "4.3.0",
    "nuxt-auth-utils": "0.5.29",
    "otplib": "^13.3.0",
    "pinia": "^3.0.4",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "tailwindcss": "^4.2.1",
    "uuid": "^13.0.0",
    "vooks": "^0.2.12",
    "vue": "^3.5.29",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "autoprefixer": "^10.4.27",
    "baseline-browser-mapping": "^2.10.0",
    "eslint": "^10.0.2",
    "eslint-plugin-oxlint": "^1.50.0",
    "nuxtjs-naive-ui": "1.0.2",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "oxlint-tsgolint": "^0.15.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "unplugin-vue-components": "^31.0.0",
    "vue-tsc": "^3.2.5"
  },
  "overrides": {
    "@koishijs/loader": "npm:@mrlingxd/loader@^4.6.8-fix.1"
  },
  "apiVersion": "0.0.1",
  "minimumClientVersion": "0.0.1",
  "patchedDependencies": {
    "nuxt-auth-utils@0.5.26": "patches/nuxt-auth-utils@0.5.26.patch"
  }
}
